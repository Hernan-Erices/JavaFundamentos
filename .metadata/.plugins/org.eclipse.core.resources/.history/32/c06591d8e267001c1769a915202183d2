package EjerciciosBasicos;

import java.util.Arrays;

public class Ejercicio2Impares {

	public static void main(String[] args) {
		//2 - Imprimir los Números Impares Entre 1 - 255
		
		int[] generador = new int [255];
		for(int i=0;i<=254;i++) {
			generador[i] = (int)(Math.random()*45+55);
			 if((i%2)!=0) {
				 System.out.println("Numero impar: " + generador[i]);
			 }
		}
		
		Arrays.sort(generador);
		for (int numero:generador)
			  System.out.println("orden: " + numero);
	}

}
