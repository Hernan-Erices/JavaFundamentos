package Rompecabezas;

import java.util.Random;

public class Actividad7 {
	
	public static void actividad6() {
		
		String[] generar = new String [10];
		
		String caracteres = "abcdefghijklmnopqrstuvwxyz";
		String cadenaAleatoria = "";
		
		int length = 5;
		
		for(int i=0;i<=9;i++) {
			
			Random random = new Random();
			
			char[] texto = new char[length];
			
			for(int x = 0; x < length; x++) {
				texto[i] = caracteres.charAt(random.nextInt(caracteres.length()));
				
			}
			for(int x=0;i<generar.length;x++) {
				cadenaAleatoria += texto[x];
			}
			System.out.println(cadenaAleatoria);	
		}

		
		
		
	}

}
