package Rompecabezas;

public class Actividad4 extends PuzzleJava{
	
	public static void actividad3() {
		
		int generador[] = new int [10];
		
		for(int i=0;i<=9;i++) {
			generador[1] = (int)(Math.random()*55+100);
			
			System.out.println("Array " + i "---" + generador[i]);
			
		}
		
		
		
	}

}
