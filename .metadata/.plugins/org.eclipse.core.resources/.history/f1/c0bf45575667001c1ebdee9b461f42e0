package EjerciciosBasicos;

public class Ejercicio2Impares {

	public static void main(String[] args) {
		//2 - Imprimir los Números Impares Entre 1 - 255
		int[] generador = new int [10];
		 int i=0;
		 for(i=0;i<=255;i++) {
			 if((i%2)!=0) {
				 System.out.println("Numero impar: " +i);
			 }
		 }
	}

}
